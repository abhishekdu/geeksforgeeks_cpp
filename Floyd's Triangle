#include <iostream>
using namespace std;
int main() {
    int t,n;
	cout<<"Enter no. of times you want to run it ";
	cin>>t;
	
	for(int i=0;i<t;i++)
	{
	   cout<<"Enter no. of level for Floyd's triangle";
	    cin>>n;
	    int value=0,count=0;
	    for(int j=1;j<=n;j++)
	    {
	        count=0;
	        while(count<j)
	        {
	            ++count;
	            ++value;
	            cout<<value<<" ";
	        }
	        cout<<"\n";
	    }
	}
	return 0;
}
